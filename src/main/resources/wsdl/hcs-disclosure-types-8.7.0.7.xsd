									<xs:documentation>Количество поступивших претензий</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:nonNegativeInteger">
										<xs:maxInclusive value="9999"/>
										<xs:minInclusive value="0"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="Successful">
								<xs:annotation>
									<xs:documentation>Количество удовлетворенных претензий</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:nonNegativeInteger">
										<xs:maxInclusive value="9999"/>
										<xs:minInclusive value="0"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="Denied">
								<xs:annotation>
									<xs:documentation>Количество претензий, в удовлетворении которых отказано</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:nonNegativeInteger">
										<xs:maxInclusive value="9999"/>
										<xs:minInclusive value="0"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="RecalculationSum" type="base:SmallMoneyPositiveType">
								<xs:annotation>
									<xs:documentation>Сумма произведенного перерасчета</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="WorkOnClaims" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Информация о ведении претензионно-исковой работы в отношении потребителей-должников</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ClaimsSent">
								<xs:annotation>
									<xs:documentation>Направлено претензий потребителям-должникам</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:nonNegativeInteger">
										<xs:maxInclusive value="9999"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="LawsuitSent">
								<xs:annotation>
									<xs:documentation>Направлено исковых заявлений</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:nonNegativeInteger">
										<xs:maxInclusive value="9999"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="ClaimsMoneyRecieved" type="base:SmallMoneyPositiveType">
								<xs:annotation>
									<xs:documentation>Получено денежных средств по результатам претензионно-исковой работы</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!--Базовые типы-->
	<xs:simpleType name="String1500Type">
		<xs:annotation>
			<xs:documentation>Скалярный тип. Строка не более 1500 символов.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="1500"/>
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="String1000Type">
		<xs:annotation>
			<xs:documentation>Скалярный тип. Строка не более 1000 символов.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="1000"/>
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="SubSection1ElementType">
		<xs:annotation>
			<xs:documentation>Тип подраздела</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="base:TransportGUID"/>
			<xs:element name="LastEditingDate" type="xs:date">
				<xs:annotation>
					<xs:documentation>Дата заполнения/внесения изменений</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DirectorFIOType">
		<xs:annotation>
			<xs:documentation>ФИО</xs:documentation>
		</xs:annotation>
	</xs:complexType>
	<xs:complexType name="TimesType">
		<xs:sequence>
			<xs:element name="HourFrom">
				<xs:annotation>
					<xs:documentation>"С" (час)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:minInclusive value="0"/>
						<xs:maxInclusive value="23"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="MinuteFrom">
				<xs:annotation>
					<xs:documentation>"С" (минута)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:minInclusive value="0"/>
						<xs:maxInclusive value="59"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="HourTo">
				<xs:annotation>
					<xs:documentation>"По" (час)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:minInclusive value="0"/>
						<xs:maxInclusive value="23"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="MinuteTo">
				<xs:annotation>
					<xs:documentation> "По" (минута)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:minInclusive value="0"/>
						<xs:maxInclusive value="59"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
