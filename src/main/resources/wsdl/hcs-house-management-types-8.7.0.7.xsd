	<!--Импорт ДУ (Договор на управление)-->
	<xs:element name="importContractRequest">
		<xs:annotation>
			<xs:documentation>Импорт сведений о ДУ</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="base:BaseType">
					<xs:sequence>
						<xs:element name="Contract" maxOccurs="100">
							<xs:complexType>
								<xs:sequence>
									<xs:sequence>
										<xs:element ref="base:TransportGUID"/>
										<xs:element name="ContractVersionGUID" type="base:GUIDType" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Идентификатор версии ДУ в ГИС ЖКХ (при венесении изенений в ДУ)</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
									<xs:choice>
										<xs:element name="PlacingContract">
											<xs:annotation>
												<xs:documentation>Размещение ДУ (создание или обновление сведений)</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:complexContent>
													<xs:extension base="tns:ContractType">
														<xs:sequence>
															<xs:element name="LicenseRequest" type="xs:boolean" fixed="true" minOccurs="0">
																<xs:annotation>
																	<xs:documentation>Формировать заявки в реестр лицензий, если сведения о лицензии/управляемом объекте отсутсвуют</xs:documentation>
																</xs:annotation>
															</xs:element>
														</xs:sequence>
													</xs:extension>
												</xs:complexContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="ApprovalContract" type="tns:ApprovalType">
											<xs:annotation>
												<xs:documentation>Утверждение ДУ, в отношении которого были одобрены заявки на включение управляемых объектов в РЛ</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="RollOverContract">
											<xs:annotation>
												<xs:documentation>Пролонгация ДУ</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:complexContent>
													<xs:extension base="tns:RollOverType">
														<xs:sequence>
															<xs:element name="LicenseRequest" type="xs:boolean" fixed="true" minOccurs="0">
																<xs:annotation>
																	<xs:documentation>Формировать заявки в реестр лицензий, если новые периоды домов не совпадают с периодами действия существующих лицензий. Срок управления для каждого МКД увеличивается автоматически на количество дней первого интервала управления МКД.</xs:documentation>
																</xs:annotation>
															</xs:element>
														</xs:sequence>
													</xs:extension>
												</xs:complexContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="TerminateContract">
											<xs:annotation>
												<xs:documentation>Расторжение ДУ</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:complexContent>
													<xs:extension base="tns:TerminateType">
														<xs:sequence>
															<xs:element name="ReasonRef" type="base:nsiRef">
																<xs:annotation>
																	<xs:documentation>Ссылка на НСИ "54	Причина расторжения договор" (реестровый номер 54)</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="TerminateAttachment" type="base:AttachmentType" maxOccurs="unbounded">
																<xs:annotation>
																	<xs:documentation>Основание</xs:documentation>
																</xs:annotation>
															</xs:element>
														</xs:sequence>
													</xs:extension>
												</xs:complexContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="AnnulmentContract">
											<xs:annotation>
												<xs:documentation>Аннулирование ДУ</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:complexContent>
													<xs:extension base="tns:AnnulmentType"/>
												</xs:complexContent>
											</xs:complexType>
										</xs:element>
									</xs:choice>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
		<xs:key name="importContractRequest_TransportGUIDKey">
			<xs:selector xpath=".//base:TransportGUID"/>
			<xs:field xpath="."/>
		</xs:key>
	</xs:element>
