	<xs:element name="importNotificationRequest">
		<xs:annotation>
			<xs:documentation>Импорт новости для информирования граждан</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="base:BaseType">
					<xs:sequence>
						<xs:element name="notification" maxOccurs="100">
							<xs:complexType>
								<xs:sequence>
									<xs:sequence>
										<xs:element ref="base:TransportGUID"/>
										<xs:element name="NotificationGUID" type="base:GUIDType" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Идентификатор новости  в ГИС ЖКХ </xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
									<xs:choice>
										<xs:element name="Create">
											<xs:annotation>
												<xs:documentation>Создание, редактирование новости</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Topic">
														<xs:annotation>
															<xs:documentation>Тема</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="base:LongTextType">
																<xs:maxLength value="200"/>
																<xs:minLength value="1"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="IsImportant" type="xs:boolean" fixed="true" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Высокая важность новости</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="content">
														<xs:annotation>
															<xs:documentation>Текст новости</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="base:LongTextType">
																<xs:minLength value="1"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:choice>
														<xs:annotation>
															<xs:documentation>Адресаты</xs:documentation>
														</xs:annotation>
														<xs:element name="IsAll" type="xs:boolean" fixed="true">
															<xs:annotation>
																<xs:documentation>Все дома (в адресатах)</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element name="FIASHouseGuid" type="base:GUIDType" maxOccurs="unbounded">
															<xs:annotation>
																<xs:documentation>Глобальный уникальный идентификатор дома по ФИАС</xs:documentation>
															</xs:annotation>
														</xs:element>
													</xs:choice>
													<xs:choice>
														<xs:annotation>
															<xs:documentation>Период актуальности</xs:documentation>
														</xs:annotation>
														<xs:element name="IsNotLimit" type="xs:boolean" fixed="true">
															<xs:annotation>
																<xs:documentation>Не ограничено </xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:sequence>
															<xs:element name="StartDate" type="xs:dateTime" minOccurs="0">
																<xs:annotation>
																	<xs:documentation>В период "С"</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="EndDate" type="xs:dateTime">
																<xs:annotation>
																	<xs:documentation>В период "До"</xs:documentation>
																</xs:annotation>
															</xs:element>
														</xs:sequence>
													</xs:choice>
													<xs:element name="Attachment" type="base:AttachmentType" minOccurs="0" maxOccurs="unbounded">
														<xs:annotation>
															<xs:documentation>Документы новости</xs:documentation>
