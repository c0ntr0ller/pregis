			<xs:element name="ExaminationVersionState" type="tns:ExaminationVersionStateType">
				<xs:annotation>
					<xs:documentation>Статус версии проверки.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Examination" type="tns:ExaminationType">
				<xs:annotation>
					<xs:documentation>Проверка.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ExportPrecept" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Предписание.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="PreceptGuid" type="base:GUIDType">
							<xs:annotation>
								<xs:documentation>Идентификатор предписания в ГИС ЖКХ.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="PreceptState" type="tns:PreceptStateType">
							<xs:annotation>
								<xs:documentation>Статус предписания</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Precept" type="tns:PreceptType">
