			<xs:documentation>Ответ на запрос на экспорт проверок.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="base:BaseType">
					<xs:choice>
						<xs:element name="exportExaminationResult" type="tns:exportExaminationResultType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Эксортируемая проверка.</xs:documentation>
