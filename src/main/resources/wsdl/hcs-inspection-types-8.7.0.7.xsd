				</xs:complexType>
			</xs:element>
			<xs:element name="ExportOffence" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Протокол.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="OffenceGuid" type="base:GUIDType">
							<xs:annotation>
								<xs:documentation>Идентификатор протокола в ГИС ЖКХ.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="OffenceState" type="tns:OffenceStateType">
							<xs:annotation>
								<xs:documentation>Статус протокола.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Offence" type="tns:OffenceType">
