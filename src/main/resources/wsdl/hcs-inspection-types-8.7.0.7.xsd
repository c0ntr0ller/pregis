		<xs:annotation>
			<xs:documentation>Возврат статуса отправленного сообщения</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="base:BaseAsyncResponseType">
					<xs:choice minOccurs="0">
						<xs:element ref="base:ErrorMessage"/>
						<xs:element name="CommonResult" type="base:CommonResultType" maxOccurs="unbounded"/>
						<xs:element name="exportInspectionPlanResult" type="tns:exportInspectionPlanResultType" maxOccurs="unbounded"/>
						<xs:element name="exportExaminationResult" type="tns:exportExaminationResultType" maxOccurs="unbounded"/>
					</xs:choice>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="CancelledInfoType">
		<xs:annotation>
			<xs:documentation>Сведения об отмене документа.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Reason" type="tns:String2000Type">
				<xs:annotation>
					<xs:documentation>Причина отмены документа.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Date" type="xs:date">
				<xs:annotation>
					<xs:documentation>Дата отмены документа.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Number" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Номер решения об отмене.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tns:String255Type">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CancelledInfoWithAttachmentsType">
		<xs:annotation>
			<xs:documentation>Сведения об отмене документа с приложением подтверждающих документов.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="tns:CancelledInfoType">
				<xs:sequence>
					<xs:element name="Attachments" type="base:AttachmentType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Подтверждающие документы. Поле является обязательным, если поле IsJudgementDecision=true</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="IsJudgementDecision" type="xs:boolean">
						<xs:annotation>
							<xs:documentation>По решению суда</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
</xs:schema>
